// src/App.js
import React from 'react';
import { BrowserRouter as Router, Route, Switch } from 'react-router-dom';
import ProductList from './components/ProductList';

const App = () => {
    return (
        <Router>
            <Switch>
                <Route path="/products" component={ProductList} />
            </Switch>
        </Router>
    );
};

export default App;
